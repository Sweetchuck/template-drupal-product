    "autoload-dev": {
        "psr-4": {
            "Sweetchuck\\TemplateDrupalProduct\\Tests\\": "tests/src/",
            "Drush\\Commands\\app\\": "drush/Commands/custom/app/"
        }
    },
    "scripts": {
        "clean": [
            "@clean:backend",
            "@clean:frontend",
            "@clean:translations"
        ],
        "clean:backend": [
            "rm -rf vendor/",
            "rm -rf docroot/core/ docroot/modules/contrib/ docroot/profiles/contrib/ docroot/themes/contrib/ docroot/libraries/",
            "rm -rf drush/Commands/contrib/"
        ],
        "clean:frontend": [
            "rm -rf docroot/themes/custom/*/node_modules/",
            "rm -rf docroot/themes/custom/*/bootstrap/",
            "rm -rf docroot/themes/custom/*/css/",
            "rm -rf docroot/themes/custom/*/dist/"
        ],
        "clean:translations": [
            "git clean -X -f -- sites/all/translations"
        ],
        "reset": [
            "@reset:cache",
            "@reset:files",
            "@reset:db"
        ],
        "reset:cache": [
            "drush --ansi --config=drush --yes @app.local cache:rebuild || true"
        ],
        "reset:files": [
            "rm -rf docroot/sites/*/files/*",
            "rm -rf sites/*/php_storage/twig/*",
            "rm -rf sites/*/private/*",
            "rm -rf sites/*/temporary/*"
        ],
        "reset:db": [
            "drush --ansi --config=drush --yes @app.local sql:drop"
        ],
        "site:install:local:default": [
            "@reset",
            "drush --ansi --config=drush @app.local site:install appp --existing-config --yes",
            "drush --ansi --config=drush @app.local config-split:import local --yes",
            "drush --ansi --config=drush @app.local marvin:toggle-modules --define=marvin.environment=local -vvv",
            "drush --ansi --config=drush @app.local marvin:migrate default"
        ],
        "site:install:local:dummy": [
            "@reset",
            "drush --ansi --config=drush @app.local site:install appp --existing-config --marvin-migrate='' --yes",
            "drush --ansi --config=drush @app.local config-split:import local --yes",
            "drush --ansi --config=drush @app.local marvin:toggle-modules --define=marvin.environment=local -vvv",
            "drush --ansi --config=drush @app.local marvin:migrate dummy"
        ],
        "site:install:prod:default": [
            "@reset",
            "drush --ansi --config=drush @app.local site:install appp --existing-config --yes"
        ],
        "site:install:prod:dummy": [
            "@reset",
            "drush --ansi --config=drush @app.local site:install appp --existing-config --yes"
        ]
    },
    "scripts-descriptions": {
        "clean": "Combines <comment>@clean:backend</comment> and <comment>@clean:frontend</comment>",
        "clean:backend": "Deletes all backend related third-party and generated codes",
        "clean:frontend": "Deletes all frontend related third-party and generated codes",
        "clean:translations": "Deletes all third-party *.po files",
        "reset": "Combines <fg=yellow>@reset:search</>, <fg=red>@reset:files</> and <fg=red>@reset:db</>",
        "reset:files": "<fg=red>Deletes</> all the files in the <info>public</info> and the <info>private</info> directories.",
        "reset:db": "<fg=red>Drops</> all the table in the <info>default database</info>.",
        "site:install:local:default": "Runs <info>`drush site:install`</info>; Toggle modules for <info>local</info> environment; Import <info>local</info> config split; Migrates <info>default</info> contents",
        "site:install:local:dummy": "Runs <info>`drush site:install`</info>; Toggle modules for <info>local</info> environment; Import <info>local</info> config split; Migrates <info>dummy</info> contents",
        "site:install:prod:default": "Runs <info>`drush site:install`</info>; Toggle modules for <info>prod</info> environment; Migrates <info>default</info> contents",
        "site:install:prod:dummy": "Runs <info>`drush site:install`</info>; Toggle modules for <info>prod</info> environment; Migrates <info>dummy</info> contents"
    },
    "extra": {
        "patches": {
            "drupal/core": [
                {
                    "description": "PoStreamReader::getSeek",
                    "url": "https://www.drupal.org/files/issues/2023-02-20/drupal-po_stream_reader_seek-3126929-45.patch",
                    "sha256": "b39da0e1ee6a2eff1deaf5f2a3532d7dd09489ce58e2f1838910d6d6a9397f6f",
                    "extra": {
                        "issue-tracker-url": "https://www.drupal.org/project/drupal/issues/3126929"
                    }
                }
            ],
            "drupal/drupal-extension": [
                {
                    "description": "Behat config schema - extendable selectors and text",
                    "url": "https://patch-diff.githubusercontent.com/raw/jhedstrom/drupalextension/pull/640.patch",
                    "sha256": "0c6759fe79625b3c0893418cacef8a6c0bde5256207efcc928a772aaa974a156",
                    "extra": {
                        "issue-tracker-url": "https://github.com/jhedstrom/drupalextension/issues/493"
                    }
