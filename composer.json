                    "mode": "skip"
                },

                "[web-root]/sites/default/.gitignore": {
                    "mode": "skip"
                },
                "[web-root]/sites/default/default.services.yml": {
                    "mode": "skip"
                },
                "[web-root]/sites/default/default.settings.php": {
                    "mode": "skip"
                },
                "[web-root]/sites/default/example.settings.php": {
                    "mode": "skip"
                },
                "[web-root]/sites/example.settings.local.php": {
                    "mode": "skip"
                },
                "[web-root]/sites/example.sites.php": {
                    "mode": "skip"
                },
                "[web-root]/sites/development.services.yml": {
                    "mode": "skip"
                },
                "[web-root]/sites/.gitignore": {
                    "mode": "skip"
                },
                "[web-root]/sites/README.md": {
                    "mode": "skip"
                },
                "[web-root]/sites/README.txt": {
                    "mode": "skip"
                },

                "[web-root]/.csslintrc": {
                    "mode": "skip"
                },
                "[web-root]/.editorconfig": {
                    "mode": "skip"
                },
                "[web-root]/.eslintignore": {
                    "mode": "skip"
                },
                "[web-root]/.eslintrc.json": {
                    "mode": "skip"
                },
                "[web-root]/.gitattributes": {
                    "mode": "skip"
